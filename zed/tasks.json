[
    {
        "label": "Python: run",
        "command": "python3",
        "args": ["$ZED_FILE"],
        "use_new_terminal": false,
        "allow_concurrent_runs": true
    },
    {
        "label": "Go: run",
        "command": "go",
        "args": ["run", "$ZED_FILE"],
        "use_new_terminal": false,
        "allow_concurrent_runs": true
    },
    {
        "label": "Create temp virtual environment",
        "command": "VIRTUALENVWRAPPER_PYTHON=$(which python3)source $(which virtualenvwrapper.sh) && mktmpenv && $SHELL",
        "use_new_terminal": true,
        "allow_concurrent_runs": true,
        "reveal": "always"
    }
]
